  keywords = {Depression,Machine learning,CANBIND,Antidepressant,Outcome,Prediction,Statistical learning},
  pages = {94-102},
  file = {C:\\Users\\jjnun\\Documents\\Zotero\\storage\\98Y58FZ4\\Iniesta et al. - 2016 - Combining clinical variables to optimize predictio.pdf;C:\\Users\\jjnun\\Documents\\Zotero\\storage\\WMRVSIDL\\S0022395616300541.html}
