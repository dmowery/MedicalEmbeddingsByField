  doi = {10.1016/j.jpsychires.2016.03.016},
  author = {Iniesta, Raquel and Malki, Karim and Maier, Wolfgang and Rietschel, Marcella and Mors, Ole and Hauser, Joanna and Henigsberg, Neven and Dernovsek, Mojca Zvezdana and Souery, Daniel and Stahl, Daniel and Dobson, Richard and Aitchison, Katherine J. and Farmer, Anne and Lewis, Cathryn M. and McGuffin, Peter and Uher, Rudolf},
  month = jul,
