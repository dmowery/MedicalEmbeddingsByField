@article{vodermaierAnxietyDiagnosisPredicts2017,
  title = {Anxiety {{After Diagnosis Predicts Lung Cancer}}\textendash{{Specific}} and {{Overall Survival}} in {{Patients With Stage III Non}}\textendash{{Small Cell Lung Cancer}}: {{A Population}}-{{Based Cohort Study}}},
