  pages = {737-750},
  file = {C:\\Users\\jjnun\\Documents\\Zotero\\storage\\LPQ8N4ZG\\Telepak et al. - 2014 - Psychosocial factors and mortality in women with e.pdf},
  pmid = {24152380},
  pmcid = {PMC4061273}
